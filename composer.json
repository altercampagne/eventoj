{
    "type": "project",
    "license": "MIT",
    "name": "altercampagne/eventoj",
    "description": "Altercampagne's booking system",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-xsl": "*",
        "aws/aws-sdk-php": "^3.299",
        "babdev/pagerfanta-bundle": "^4.4",
        "barbieswimcrew/zip-code-validator": "^3.1",
        "doctrine/doctrine-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^3.3",
        "doskyft/helloasso-php": "dev-main",
        "dunglas/doctrine-json-odm": "^1.4",
        "geocoder-php/photon-provider": "^0.10.0",
        "martin-georgiev/postgresql-for-doctrine": "^3.0",
        "odolbeau/phone-number-bundle": "^4.0",
        "pagerfanta/doctrine-orm-adapter": "^4.5",
        "pagerfanta/twig": "^4.5",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^2.0",
        "sentry/sentry-symfony": "^5.0",
        "stof/doctrine-extensions-bundle": "^1.11",
        "symfony/asset": "^7.4",
        "symfony/asset-mapper": "^7.4",
        "symfony/console": "^7.4",
        "symfony/doctrine-messenger": "^7.4",
        "symfony/dotenv": "^7.4",
        "symfony/expression-language": "^7.4",
        "symfony/flex": "^2",
        "symfony/form": "^7.4",
        "symfony/framework-bundle": "^7.4",
        "symfony/http-client": "^7.4",
        "symfony/intl": "^7.4",
        "symfony/mailer": "^7.4",
        "symfony/mime": "^7.4",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "^7.4",
        "symfony/process": "^7.4",
        "symfony/property-access": "^7.4",
        "symfony/property-info": "^7.4",
        "symfony/rate-limiter": "^7.4",
        "symfony/runtime": "^7.4",
        "symfony/security-bundle": "^7.4",
        "symfony/serializer": "^7.4",
        "symfony/stimulus-bundle": "^2.15",
        "symfony/string": "^7.4",
        "symfony/translation": "^7.4",
        "symfony/twig-bundle": "^7.4",
        "symfony/uid": "^7.4",
        "symfony/ux-autocomplete": "^2.15",
        "symfony/ux-twig-component": "^2.13",
        "symfony/validator": "^7.4",
        "symfony/web-link": "^7.4",
        "symfony/webhook": "^7.4",
        "symfony/yaml": "^7.4",
        "symfonycasts/reset-password-bundle": "^1.20",
        "symfonycasts/sass-bundle": "^0.8",
        "symfonycasts/verify-email-bundle": "^1.14",
        "twbs/bootstrap": "^5.3",
        "twig/cssinliner-extra": "^3.7",
        "twig/extra-bundle": "^3.8",
        "twig/inky-extra": "^3.7",
        "twig/intl-extra": "^3.8",
        "twig/string-extra": "^3.8",
        "twig/twig": "^2.12|^3.0",
        "willdurand/geocoder-bundle": "^5.20"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^7.4",
            "docker": true
        }
    },
    "require-dev": {
        "bab/tested-routes-checker-bundle": "^1.0",
        "dama/doctrine-test-bundle": "^8.4",
        "doctrine/doctrine-fixtures-bundle": "^4.0",
        "fakerphp/faker": "^1.23",
        "phpunit/phpunit": "^12.0",
        "symfony/browser-kit": "^7.4",
        "symfony/css-selector": "^7.4",
        "symfony/debug-bundle": "^7.4",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^7.4",
        "symfony/stopwatch": "^7.4",
        "symfony/web-profiler-bundle": "^7.4",
        "zenstruck/foundry": "^2.3"
    }
}
